<h1>Public::Customers#show</h1>
<p>Find me in app/views/public/customers/show.html.erb</p>
<div class="container">
  <table class="table table-bordered">
    <tbody>
      <tr>
        <th scope="row" class="bg-light">氏名</th>
        <td><%= @customer.last_name %>　<%= @customer.first_name %></td>
      </tr>
      <tr>
        <th scope="row" class="bg-light">カナ</th>
        <td><%= @customer.last_name_kana %>　<%= @customer.first_name_kana %></td>
      </tr>
      <tr>
        <th scope="row" class="bg-light">郵便番号</th>
        <td><%= @customer.postal_code %></td>
      </tr>
      <tr>
        <th scope="row" class="bg-light">住所</th>
        <td><%= @customer.address %></td>
      </tr>
      <tr>
        <th scope="row" class="bg-light">電話番号</th>
        <td><%= @customer.phone_number %></td>
      </tr>
      <tr>
        <th scope="row" class="bg-light">メールアドレス</th>
        <td><%= @customer.email %>></td>
      </tr>
    </tbody>
  </table>
</div>